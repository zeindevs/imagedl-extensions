{
  "manifest_version": 2,
  "name": "Image Downloader",
  "description": "Image Downloader",
  "minimum_chrome_version": "72",
  "version": "1.0.0",
  "key": "",
  "background": {
    "presistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "vite.png",
    "default_popup": "index.html"
  },
  "options_page": "options.html",
  "icons": {
    "128": "vite.png",
    "48": "vite.png",
    "16": "vite.png"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
